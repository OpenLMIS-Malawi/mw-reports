	<queryString language="plsql">
		<![CDATA[WITH RECURSIVE findChildZones AS (
  SELECT * FROM referencedata.geographic_zones WHERE name = $P{district}::varchar
  UNION ALL
  SELECT referencedata.geographic_zones.* FROM referencedata.geographic_zones JOIN findChildZones ON findChildZones.id = referencedata.geographic_zones.parentId
)
SELECT
  referencedata.facilities.name AS facilityname,
  referencedata.orderable_display_categories.displayname AS category,
  referencedata.orderables.code AS productcode,
  referencedata.orderables.fullproductname AS productname,
  dispensingunit.value AS unitofissue,
  requisition.requisition_line_items.stockonhand AS balanceonhand,
  requisition.requisition_line_items.totalconsumedquantity AS quantityused,
  requisition.requisition_line_items.totalreceivedquantity AS quantityreceived,
  requisition.requisition_line_items.requestedquantity AS quantityrequired,
  requisition.requisition_line_items.approvedquantity AS approvedquantity,
  referencedata.geographic_zones.name AS geographiczone,
  requisition.requisitions.emergency AS ordertype
FROM
  requisition.requisition_line_items
  JOIN requisition.requisitions ON requisition.requisition_line_items.requisitionid = requisition.requisitions.id AND (requisition.requisitions.status in ('SUBMITTED', 'AUTHORIZED', 'IN_APPROVAL', 'IN_APPROVAL', 'APPROVED', 'RELEASED'))
  JOIN referencedata.orderables ON requisition.requisition_line_items.orderableid = referencedata.orderables.id
  JOIN referencedata.program_orderables ON referencedata.orderables.id = referencedata.program_orderables.orderableid and referencedata.program_orderables.programid = requisition.requisitions.programid
  JOIN referencedata.orderable_display_categories ON referencedata.program_orderables.orderabledisplaycategoryid = referencedata.orderable_display_categories.id
  LEFT JOIN referencedata.dispensable_attributes AS dispensingunit ON dispensingunit.key = 'dispensingUnit' AND dispensingunit.dispensableid = orderables.dispensableid
  JOIN referencedata.programs ON requisition.requisitions.programid = referencedata.programs.id and referencedata.programs.name = $P{program}::varchar
  JOIN referencedata.facilities ON requisition.requisitions.facilityid = referencedata.facilities.id AND referencedata.facilities.active = true
  JOIN referencedata.processing_periods ON requisition.requisitions.processingperiodid = referencedata.processing_periods.id AND referencedata.processing_periods.name = $P{period}::varchar
  JOIN referencedata.geographic_zones ON referencedata.facilities.geographiczoneid = referencedata.geographic_zones.id AND ($P{district}::varchar IS NULL OR referencedata.geographic_zones.id IN (SELECT id FROM findChildZones))
  JOIN referencedata.geographic_levels ON referencedata.geographic_zones.levelid = referencedata.geographic_levels.id
WHERE
  (stockonhand > 0 OR totalconsumedquantity > 0 OR totalreceivedquantity > 0 OR requestedquantity > 0 OR approvedquantity > 0)
ORDER BY
  referencedata.facilities.name DESC,
  referencedata.orderable_display_categories.displayorder ASC,
  referencedata.program_orderables.displayorder ASC,
  referencedata.orderables.fullproductname DESC]]>
	</queryString>
